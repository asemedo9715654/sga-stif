@model sga_stif.ViewModel.MenuAcao.DadosComponente

@if (Model.Preenchido == true)
{
    <input id="@Model.IdMenuAcao" checked type="checkbox" onchange="EnviaDados(@Model.IdAcao,@ViewBag.IdMenu)" />
}
else
{
    <input id="@Model.IdMenuAcao" type="checkbox" onchange="EnviaDados(@Model.IdAcao,@Model.IdMenu)" />
}